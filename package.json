{
  "name": "@magellanic/magellanic-sdk",
  "version": "1.0.6",
  "description": "SDK to connect to Magellanic",
  "scripts": {
    "prepare": "yarn build",
    "prepack": "yarn build",
    "build": "rimraf dist && concurrently 'yarn:build:cjs' 'yarn:build:es' 'yarn:build:types' 'yarn:copy:wasm'",
    "build:cjs": "tsc -p tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "build:es": "tsc -p tsconfig.es.json && echo '{\"type\":\"module\"}' > dist/es/package.json",
    "build:types": "tsc -p tsconfig.types.json",
    "docs": "typedoc --excludePrivate --plugin typedoc-plugin-markdown src/index.ts",
    "copy:wasm": "cpx 'src/crypto/crypto-ext.wasm' 'dist/crypto'"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/es/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/**"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "axios-retry": "^3.8.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.4.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "concurrently": "^8.2.0",
    "cpx": "^1.5.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.1",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.1.6"
  }
}
